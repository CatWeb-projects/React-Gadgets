{
  "name": "initial-start-projects",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-slick": "^0.23.4",
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "eslint-config-airbnb": "^18.2.1",
    "estafette": "^1.0.10",
    "estafette-intl": "^1.0.2",
    "estafette-router": "^2.2.1",
    "express": "^4.17.1",
    "mongodb": "^4.1.1",
    "mongoose": "^5.12.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.7",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-inlinesvg": "^2.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "typescript": "^3.9.9",
    "vscode-eslint-debug": "^1.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lerna:lint:js": "lerna run lint:js:fix",
    "format": "npx prettier --write \"src/**/*.{js,jsx,ts,tsx,json,scss}\"",
    "server": "nodemon ./src/server/server.ts"
  },
  "eslintConfig": {
    "extends": "react-app",
    "configFile": "path.join(__dirname, 'eslintrc.json')"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint -c .eslintrc --fix --color"
    ],
    "src/**/*.{js,jsx,scss}": [
      "npx prettier --write -c .prettierrc"
    ]
  }
}
